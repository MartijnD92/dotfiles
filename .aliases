#!/usr/bin/env zsh

# ALIASES
alias md="mkdir"
alias st="speedtest-cli"
alias diff="diff -u --color"
alias v="lvim"
alias cl="clear"
alias py="python3"
alias zsh="v ~/.zshrc"
alias szsh="source ~/.zshrc"
alias c="tr -d '\n' | pbcopy" # Trim new lines and copy to clipboard
alias urlencode='python3 -c "import sys, urllib.parse as ul; print(ul.quote_plus(sys.argv[1]));"' # URL-encode strings
